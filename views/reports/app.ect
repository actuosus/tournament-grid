<html>
<head>
    <link rel="stylesheet" href="<%- @staticDomain %>/agamapro-virtus.pro-ebb4d51390f8/css/style.css" media="screen,projection"/>
    
    <style type="text/css">
        body {
            background: black;
        }
    </style>
</head>
    <body>
        <div class="i-wrapper cf">
            <div class="i-container cf">
                <div class="b-content">
                    <div id="content"></div>
                    <script type="text/javascript">
                        window.currentReportId = '<%- @doc.id %>';
                        window.debug = true;
                    </script>

                    <% if @node_env is 'production': %>
                    <link rel="stylesheet" href="<%- @staticDomain %>/bundle/stylesheets/app.css" />
                    <% else: %>
                    <link rel="stylesheet" href="/app/stylesheets/main.css" />
                    <% end %>
                    <link rel="icon" type="image/x-icon" href="<%- @staticDomain %>/favicon.ico" />

                    <% if @node_env is 'production': %>
                    <script data-main="/bundle/app.js" src="/vendor/scripts/require.js"></script>
                    <% else: %>
                    <script data-main="/app/main.js" src="/vendor/scripts/require.js"></script>
                    <% end %>

                    <script type="text/javascript" id="grid-config">
                        window.grid = {};
                        window.grid.config = {
                            languages: <%- JSON.stringify(@languages) %>

                        };
                    </script>

                    <!--<% if @node_env is 'production': %>-->
                    <!--<script type="text/javascript" src="<%- @staticDomain %>/bundle/app.js"></script>-->
                    <!--<% else: %>-->
                    <!--<script type="text/javascript" src="/app/main.js"></script>-->
                    <!--<% end %>-->

                    <script type="text/x-handlebars" data-template-name="stages/new">
                        {{view App.StageForm}}
                    </script>

                    <script type="text/x-handlebars" data-template-name="stages">
                        <div class="block-container named-container">
                            <p class="ember-view b-profileBlock__title">
                                {{loc '_tournament_results_table'}}
                            </p>

                            <div class="stage-view">
                                <div class="i-listsTabs i-listsTabs_bd">
                                    {{log controller.content}}
                                    <ul class="b-listsTabs">
                                        {{#each stage in controller}}
                                            {{#linkTo stage stage tagName="li" classNames="item"}}{{stage.name}}{{/linkTo}}
                                        {{/each}}
                                        {{#if App.isEditingMode}}
                                            {{#linkTo stages.new tagName="li" classNames="item"}}+{{/linkTo}}
                                        {{/if}}
                                    </ul>
                                </div>

                                {{outlet}}
                            </div>
                        </div>
                    </script>

                    <script type="text/x-handlebars" data-template-name="stage">
                        <div class="grid">
                            {{#each matches}}
                                {{view App.MatchGridItemView contentBinding=this}}
                            {{/each}}
                        </div>
                    </script>

                    <script type="text/x-handlebars" data-template-name="matches/new">
                        {{view App.MatchGridItemView}}
                    </script>

                    <script type="text/x-handlebars" data-template-name="application"></script>
                </div>
            </div>
        </div>
    </body>
</html>
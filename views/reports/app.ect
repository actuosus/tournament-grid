<html lang="<%- @language %>">
<head>
    <link rel="stylesheet" href="<%- @staticDomain %>/agamapro-virtus.pro-ebb4d51390f8/css/style.css" media="screen,projection"/>

    <title><%- @__('Report') %> <%- @doc.title %></title>
    <style type="text/css">
        body {
            background: #171717;
        }
        .i-wrapper {
            padding-top: 0;
        }
        .i-container {
            background: none;
        }
    </style>
</head>
    <body>
        <div class="i-wrapper cf">
            <div class="i-container cf">
                <div class="b-content">
                    <div id="content">
                        <div class="block-container named-container collapsed">
                            <p class="b-profileBlock__title close"><%- @__('Tournament results table') %></p>
                            <img src="/images/loader.gif" class="loader"/>
                            <div class="status-text" id="loader-status"><%- @__('Loading') %>â€¦</div>
                            <div class="error-text" id="loader-error"></div>
                        </div>
                    </div>
                    <script type="text/javascript">
                        window.DEBUG = true;
                        window.grid = {
                            reportId: '<%- @doc.id %>',
                            //        reportId: '33664',
                            //        reportId: '38272',
                            //        reportId: '107852',
                            //        reportId: '113576',
                            config: {
                                rootElement: '#content',
                                authUrl: '/auth',
                                languages: <%- JSON.stringify(@languages) %>,
                                socket: {
                                    url: 'http://v3.virtuspro.org:33891/'
                                }
                            }
                        };
                    </script>
                    <% if @node_env is 'production': %>
                    <link rel="stylesheet" href="<%- @staticDomain %>/bundle/stylesheets/app.css" />
                    <% else: %>
                    <link rel="stylesheet" href="/app/stylesheets/main.css" />
                    <% end %>
                    <link rel="stylesheet" href="/vendor/styles/flags.css" />
                    <% if @node_env is 'production': %>
                    <script data-main="/bundle/app.js" src="/vendor/scripts/require.min.js"></script>
                    <% else: %>
                    <script data-main="/app.main.js" src="/vendor/scripts/require.min.js"></script>
                    <% end %>
                </div>
            </div>
        </div>
    </body>
</html>
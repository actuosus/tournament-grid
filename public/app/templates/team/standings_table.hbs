<thead>
    <tr>
        <th class="header-item header-item-position" title="{{loc '_place_position'}}">#</th>
        <th class="header-item header-item-name">{{loc '_title'}}</th>
        <th class="header-item header-item-matches-played" title="{{loc '_games'}}">{{loc '_games_abbr'}}</th>
        <th class="header-item header-item-wins"  title="{{loc '_wins'}}">{{loc '_wins_abbr'}}</th>
        <th class="header-item header-item-draws"  title="{{loc '_draws'}}">{{loc '_draw_abbr'}}</th>
        <th class="header-item header-item-losses"  title="{{loc '_losses'}}">{{loc '_losses_abbr'}}</th>
        <th class="header-item header-item-points"  title="{{loc '_points'}}">{{loc '_points_abbr'}}</th>
        <th class="header-item header-item-difference"  title="{{loc '_difference'}}">{{loc '_difference_abbr'}}</th>
    </tr>
</thead>
{{#each view.content}}
    {{view App.TeamStandingsTableItemView contentBinding=this}}
{{/each}}